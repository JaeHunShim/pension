<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.mapper.ReserMapper">
	<select id="listAll" resultType="ReserVO">
		select * from board;
	</select>
	<insert id="create">
		insert into reservation(roomN,room,max,fee,addfee,choice) values(#{roomN},#{room},#{max},#{fee},#{addfee},#{choice})
	</insert>
</mapper>